\newcommand{\GetCommitteeMember}[1]{%
    \IfStrEqCase{#1}{%
        {1}{\@CommitteeMemberAName}%
        {2}{\@CommitteeMemberBName}%
        {3}{\@CommitteeMemberCName}%
        {4}{\@CommitteeMemberDName}%
        {5}{\@CommitteeMemberEName}%
    }[]%
}%
% ============================================================================ %
\newcommand{\SectionCoverPage}[1]{%
    \thispagestyle{empty}%
    \begin{center}%
	    \vspace*{\fill}%
	    \Huge\textbf{#1}%
	    \vspace{1.50in}%
	    \vspace*{\fill}%
    \end{center}%
    \clearpage{\pagestyle{empty}\cleardoublepage}%
}%
