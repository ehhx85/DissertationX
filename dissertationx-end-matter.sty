\newcommand{\GetCommitteeMember}[1]{%
    \IfStrEqCase{#1}{%
        {1}{\@CommitteeMemberAName}%
        {2}{\@CommitteeMemberBName}%
        {3}{\@CommitteeMemberCName}%
        {4}{\@CommitteeMemberDName}%
        {5}{\@CommitteeMemberEName}%
    }[]%
}%
% ============================================================================ %
\newcommand{\SectionCoverPage}[1]{%
    \newpage% remove if using fancy header
    %\thispagestyle{empty}% used with fancy header
    \begin{center}%
	    \vspace*{\fill}%
	    \Huge\textbf{#1}%
	    \vspace{1.50in}%
	    \vspace*{\fill}%
    \end{center}%
    \addcontentsline{toc}{chapter}{#1}%
    %\clearpage{\pagestyle{empty}\cleardoublepage}% used with fancy header (make into optional command?)
}%
